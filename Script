TextButton_4.Position = UDim2.new(0.00200000009, 0, 0.159999992, 0)
TextButton_4.Size = UDim2.new(0, 184, 0, 64)
TextButton_4.Font = Enum.Font.SourceSans
TextButton_4.Text = "Smite (z)"
TextButton_4.TextColor3 = Color3.fromRGB(0, 0, 0)
TextButton_4.TextSize = 13.000
TextButton_4.MouseButton1Down:connect(function()
local condition = true
while condition do
local A_1 = "Death Smite"
local Event = game:GetService("ReplicatedStorage").UseSpell
Event:FireServer(A_1)
wait(2)
end
end)

TextButton_9.Parent = ScrollingFrame
TextButton_9.BackgroundColor3 = Color3.fromRGB(178,34,34)
TextButton_9.Position = UDim2.new(0.00200000009, 0, 0.24000000, 0)
